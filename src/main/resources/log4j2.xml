<configuration status="WARN">
    <appenders>
        <console name="out" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </console>
    </appenders>

    <loggers>
        <root level="info">
            <appender-ref ref="out" level="info"/>
        </root>
    </loggers>
</configuration>